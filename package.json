{
  "name": "fern-monorepo",
  "scripts": {
    "bi": "cd ./packages/icons && rmdir /S /Q \"dist\" && tsc",
    "br": "cd ./packages/react && rmdir /S /Q \"dist\" && tsc",
    "bu": "cd ./packages/util && rmdir /S /Q dist && tsc && xcopy .\\dist ..\\util-react\\src\\util /i /Y",
    "bur": "cd ./packages/util-react && rmdir /S /Q \"dist\" && tsc",
    "build": "pnpm bi && pnpm br && pnpm bu && pnpm bur"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "^18.0.27",
    "concurrently": "^7.6.0"
  }
}