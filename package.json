{
  "name": "fern-monorepo",
  "private": "true",
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "ir": "cd ./packages/react && pnpm i",
    "iur": "cd ./packages/util-react && pnpm i",
    "fern-install": "concurrently \"pnpm i\" \"pnpm ir\" \"pnpm iur\"",
    "fern-build": "rollup -c"
  },
  "dependencies": {
    "next": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "concurrently": "^7.6.0",
    "rollup": "^3.12.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  }
}