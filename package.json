{
  "name": "fernui",
  "scripts": {
    "r-icons": "if exist .\\packages\\icons\\_dist rmdir /S /Q .\\packages\\icons\\_dist",
    "r-react": "if exist .\\packages\\react\\_dist rmdir /S /Q .\\packages\\react\\_dist",
    "r-util": "if exist .\\packages\\util\\_dist rmdir /S /Q .\\packages\\util\\_dist",
    "r-all": "concurrently \"pnpm r-icons\" \"pnpm r-react\" \"pnpm r-util\"",
    "b-icons": "cd ./packages/icons && tsc",
    "b-react": "cd ./packages/react && pnpm i && tsc",
    "b-util": "cd ./packages/util && tsc",
    "b-ts": "concurrently \"pnpm b-icons\" \"pnpm b-react\" \"pnpm b-util\"",
    "build": "pnpm r-all && pnpm b-ts && rollup -c && pnpm r-all"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.32",
    "concurrently": "^8.0.1",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.0.3"
  }
}